{
  "name": "ospapp",
  "version": "1.0.0",
  "description": "Osp app package information",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "build:dev": "cross-env webpackConfig=dev AppSetting=dev gulp default",
    "build:ppe": "cross-env webpackConfig=prod AppSetting=ppe gulp default ",
    "build:prod": "cross-env webpackConfig=prod AppSetting=prod gulp default",
    "start": "cross-env webpackConfig=dev AppSetting=dev webpack-dev-server --config ./config/webpack.dev.js",
    "start:release": "webpack-dev-server --config ./config/server.prod.js",
    "start:withApi": "cross-env webpackConfig=dev AppSetting=devApi webpack-dev-server --config ./config/webpack.dev.js",
    "clean": "gulp clean",
    "package": "gulp package",
    "linter": "gulp watch:lint"
  },
  "jest": {
    "collectCoverage": false,
    "coverageReporters": [
      "text"
    ],
    "coverageDirectory": "./tests",
    "transform": {
      ".(tsx)$": "<rootDir>/tests/config/jest.preprocessor.js",
      ".(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "/tmp/",
      "/dist/"
    ],
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/tests/Config/",
      "/node_modules/",
      "/tmp/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "mock-local-storage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/classnames": "2.2.0",
    "@types/node": "8.0.19",
    "@types/object-hash": "0.5.28",
    "@types/query-string": "5.0.0",
    "@types/react": "16.0.1",
    "@types/react-dom": "15.5.2",
    "@types/react-highlight-words": "0.6.2",
    "@types/react-router-dom": "4.0.7",
    "@types/scroll-into-view": "1.6.5",
    "@types/xml2js": "0.4.0",
    "axios": "0.16.2",
    "babel-polyfill": "6.23.0",
    "classnames": "2.2.5",
    "es6-promise": "4.1.1",
    "highcharts": "5.0.11",
    "intro.js": "2.6.0",
    "lodash": "4.17.4",
    "mobx": "3.2.2",
    "mobx-react": "4.2.2",
    "moment": "2.18.1",
    "moment-timezone": "0.5.13",
    "object-hash": "1.1.8",
    "react": "15.6.1",
    "react-datetime": "2.8.11",
    "react-dom": "15.6.1",
    "react-highlight-words": "0.8.0",
    "react-linkify": "0.2.1",
    "react-router-dom": "4.1.2",
    "semantic-ui-react": "0.71.3",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "@types/enzyme": "2.8.6",
    "@types/jest": "20.0.7",
    "add-asset-html-webpack-plugin": "2.1.0",
    "babel-core": "6.25.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.4",
    "del": "3.0.0",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "extract-text-webpack-plugin": "3.0.0",
    "fork-ts-checker-webpack-plugin": "0.2.7",
    "gulp": "3.9.1",
    "gulp-clean-css": "3.7.0",
    "gulp-concat": "2.6.1",
    "gulp-less": "3.3.2",
    "gulp-sourcemaps": "2.6.0",
    "gulp-stylelint": "4.0.0",
    "gulp-tslint": "8.1.1",
    "gulp-util": "3.0.8",
    "gulp-zip": "4.0.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "jest-css-modules": "1.1.0",
