    "ts-loader": "2.3.2",
    "tslint": "5.5.0",
    "tslint-loader": "3.5.3",
    "tslint-microsoft-contrib": "5.0.1",
    "tsutils": "2.8.0",
    "typescript": "2.4.2",
    "webpack": "3.4.1",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.6.1",
    "webpack-notifier": "1.5.0"
  }
}
